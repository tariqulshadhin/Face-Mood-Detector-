    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/worker"
        android:label="@string/app_name"
        android:roundIcon="@drawable/worker"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.firebase.ml.vision.DEPENDENCIES"
            android:value="face" />

        <activity android:name=".Main2Activity" />
        <activity android:name=".Sad" />
        <activity android:name=".Calm" />
        <activity android:name=".Happy" />
        <activity android:name=".Songs" />
        <activity android:name=".Angree" />
        <activity android:name=".PhoneNumber" />
        <activity android:name=".VerifyCode"></activity>
    </application>

</manifest>